        "x_train = x_train / 255.0\n",
        "x_train_flat = x_train.reshape(-1, 28 * 28)\n",
        "\n",
        "x_test = x_test / 255.0\n",
        "x_test_flat = x_test.reshape(-1, 28 * 28)\n",
        "\n",
        "\n",
        "print(\"شکل اولیه x_train:\", x_train.shape)        # (60000, 28, 28)\n",
